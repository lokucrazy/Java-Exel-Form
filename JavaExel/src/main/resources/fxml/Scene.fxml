<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9.0.1" fx:controller="com.javaexel.FXMLController">
    <children>
      <Button fx:id="exp_button" layoutX="368.0" layoutY="527.0"  text="Export" />
      <TextField fx:id="name_field" layoutX="86.0" layoutY="125.0" promptText="Enter Name" />
      <TextField fx:id="cond_field" layoutX="326.0" layoutY="125.0" promptText="Enter Condition" AnchorPane.topAnchor="125.0" />
      <TextField fx:id="quan_field" layoutX="556.0" layoutY="125.0" promptText="Enter Quantity" />
      <TextArea fx:id="item_field" editable="false" layoutX="86.0" layoutY="200.0" prefHeight="302.0" prefWidth="620.0" />
      <Button fx:id="add_button" layoutX="375.0" layoutY="166.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Add" />
      <Label fx:id="title" layoutX="316.0" layoutY="32.0" text="Java Exel Form">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
